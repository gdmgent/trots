---
---
[
  {% assign posts = site.projects %}
  {% for project in projects %}
    {
      "id": "{{ project.url | slugify }}",
      "url": "{{ project.url | prepend: site.baseurl | prepend: site.url }}", 
      "title": "{{ project.title | escape }}",
      "slug": "{{ project.slug | escape }}",
      "description": "{{ project.description | escape }}",
      "content": {{ project.content | strip_html | strip_newlines | remove:  "	" | escape | remove: "\" | jsonify }}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]